                                <span> <i class="fa-solid fa-user-tie fa-2x"></i></span>
                                <div class="float-end d-block d-sm-none"><h6>MI CUENTA</h6></div>
                                <hr class="d-block d-sm-none" />
                            </div>
                        </a>
                    </li>
                </ul>
            </nav>
            <main class="col-md-9 col-lg-10 px-md-4 mx-auto">
                @RenderBody()
                <partial name="_Alertas" />
            </main>
        </div>
    </div>

